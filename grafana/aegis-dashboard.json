{
  "title": "Aegis FraudStream",
  "timezone": "browser",
  "panels": [
    {
      "type": "stat",
      "title": "Req/s",
      "targets": [
        {
          "expr": "sum(rate(aegis_requests_total[1m]))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Inference latency p95",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(aegis_infer_seconds_bucket[5m])) by (le))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Requests by status",
      "targets": [
        {
          "expr": "sum by (status)(rate(aegis_requests_total[5m]))"
        }
      ]
    }
  ],
  "schemaVersion": 37,
  "version": 1
}
